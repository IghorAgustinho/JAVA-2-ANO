
package listaexercicios1;

import java.util.Scanner;


public class ListaExercicios1 {

   
    public static void main(String[] args) {
        
        
        float media1 = (8 + 9 + 7) / 3;
        
       float media2 = (4 + 5 + 6) / 3;
       float somaMedia = media1 + media2;
       float mediaMedias = somaMedia /2;
       
       System.out.println("Media 1 :" + media1);
       System.out.println("Media 2 :" + media2);
       System.out.println("Somas das Medias :" + somaMedia);
       System.out.println("Media das Medias :" + mediaMedias);
                
    }
    
}
